{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.2.5",
        "jackbrycesmith/laravel-stripe-connect": "1.x-dev",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.3",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "lorisleiva/laravel-actions": "^1.1",
        "pbmedia/laravel-ffmpeg": "^7.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-model-status": "^1.10",
        "spatie/regex": "^1.4",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.12",
        "staudenmeir/eloquent-json-relations": "^1.4",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laracasts/cypress": "^1.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "pestphp/pest": "^0.2.2",
        "pestphp/pest-plugin-faker": "^0.2.0",
        "pestphp/pest-plugin-laravel": "^0.2.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Utils/Helper.php",
            "app/Utils/Macro.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-stripe-connect",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "composer",
            "url": "https://jbs.repo.repman.io"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
